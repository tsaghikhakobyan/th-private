package com.company;

import java.util.Random;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        int[] randomAray = new int[10];
        Random random = new Random();
        for (int i = 0; i < randomAray.length; i++) {
            randomAray[i] = random.nextInt(89) + 10;
        }
        for (int i = 0; i < randomAray.length; i++) {
            System.out.print(randomAray[i] + " ");
        }
        System.out.println("Input the number to test if an array contains that number");
        Scanner scanner = new Scanner(System.in);
        int testNumber = scanner.nextInt();
        boolean test = false;
        for (int i = 0; i < randomAray.length; i++) {
            if (testNumber == randomAray[i]) {
                test = true;
                break;
            }
        }
        if (test) {
            System.out.println("Yes contains");
        } else {
            System.out.println("Does not contain ");
        }
    }
}
